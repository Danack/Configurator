{
    "name": "intahwebz/configurator",
    "description": "Generates config files from PHP based config files and PHP and JSON data files.",
    "keywords": ["php", "config", "tool", "UTF-8"],
    "license": "MIT",
    "autoload": {
        "psr-0": {
            "Configurator": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Danack/Auryn"
        }
    ],
    "bin": [
        "bin/configurate",
        "bin/fpmconv"
    ],
    "require-dev": {
        "phpunit/phpunit": "4.4.5",
        "mikey179/vfsStream": "^1.4.0"
    },
    "require": {
        "danack/auryn": "^0.14.0",
        "danack/console": "^2.6.1"
    },
    "authors": [
        {
            "name": "Dan Ackroyd",
            "email": "Danack@basereality.com",
            "homepage": "http://www.basereality.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}